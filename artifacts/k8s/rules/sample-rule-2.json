{
  "id": "3322",
  "groups": [
    {
      "name": "memory",
      "rules": [
        {
          "alert": "HighMem",
          "expr": "process_virtual_memory_bytes > 40046016",
          "for": "3m",
          "labels": {
            "severity": "page"
          },
          "annotations": {
            "summary": "High memory use"
          }
        }
      ]
    }
  ]
}